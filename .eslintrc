{
    "rules": {
        "indent": [2,2],
        "quotes": [2,"single"],
        "linebreak-style": [2,"unix"],
        "semi": [2,"always"],
        strict: [2, "function"],
        no-eval: 2,
        no-implied-eval: 2
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
